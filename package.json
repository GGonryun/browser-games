{
  "name": "worksheets",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "check:all": "npx nx format:check && npm run check",
    "check": "npx nx affected -t lint --parallel=3 && npx nx affected -t test --parallel=3 --configuration=ci && npx nx affected -t build --parallel=3",
    "graph": "npx nx graph",
    "tools:localip": "ifconfig -u | grep 'inet ' | grep -v 127.0.0.1 | cut -d\\  -f2 | head -1",
    "test:util-time": "npx nx run util-time:test --watch",
    "test:util-arrays": "npx nx run util-arrays:test --watch",
    "serve:emojiwar": "npx nx run games-emoji-war:serve",
    "serve:puzzlewords": "npx nx run games-puzzle-words:serve",
    "serve:wordsearch": "npx nx run games-word-search:serve",
    "serve:wordpack": "npx nx run games-word-pack:serve",
    "serve:wordsmith": "npx nx run games-word-smith:serve",
    "serve:nonograms": "npx nx run games-nonograms:serve",
    "serve:solitaire": "npx nx run games-solitaire:serve",
    "env:local": "dotenv -e .env.local --",
    "serve": "concurrently --kill-others \"npm run serve:emojiwar\" \"npm run serve:puzzlewords\" \"npm run serve:wordsearch\" \"npm run serve:wordpack\" \"npm run serve:wordsmith\" \"npm run serve:nonograms\" \"npm run serve:solitaire\""
  },
